{
  "manifest_version": 3,
  "name": "Google Docs Reader",
  "version": "1.1",
  "description": "A nice extension to toggle preview mode in Google Docs, Sheets, and Slides.",
  "permissions": ["tabs"],
  "background": {
    "service_worker": "worker.js"
  },
  "action": {
    "default_title": "Toggle Preview Mode"
  },
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  }
}
